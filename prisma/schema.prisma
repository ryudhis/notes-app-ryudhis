generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}


datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") 
  directUrl = env("POSTGRES_URL_NON_POOLING") 
}

model notes {
  id        String      @id @default(uuid())
  title     String
  body   String
  createdAt DateTime @default(now())
}